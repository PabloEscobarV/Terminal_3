{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                // "src/netdata.c",
                // "src/crt_argvt.c",
                "src/str_handler.c",
                "-L../netdata",
                "-lnetdata",
                "-lreadline",
                "-L../libft",
                "-lft",
                "-L../splitter",
                "-lsplitter",
                "-L../str_ch_delete",
                "-lstr_ch_delete",
                "-L../get_app_path",
                "-lget_app_path",
                "-L../handle_in_file",
                "-lhandle_in_file",
                "-L../HashTable",
                "-lhashtable",
                "-L../varhandler",
                "-lvarhandler",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
